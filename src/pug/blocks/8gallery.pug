include ../mixins/title
- let gallery = ['gallery-1', 'gallery-2', 'gallery-3', 'gallery-1', 'gallery-2', 'gallery-3']

.gallery
  .container
    .gallery__header
      +title('Дизайн-проекты')
      p.subtitle Разработанные Ольгой Ивановой и командой Studia 54
    .gallery__list
      .swiper-wrapper
        each val in gallery
          .gallery__item.swiper-slide
            .gallery__image
              picture
                img(src=`./img/${val}.jpg`, alt="speaker", width="100%", height="100%")
